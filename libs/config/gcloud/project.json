{
    "name": "config-gcloud",
    "$schema": "../../../node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "libs/config/gcloud/src",
    "projectType": "library",
    "targets": {
        "build": {
            "executor": "@nrwl/js:tsc",
            "outputs": ["{options.outputPath}"],
            "options": {
                "outputPath": "dist/libs/config/gcloud",
                "tsConfig": "libs/config/gcloud/tsconfig.lib.json",
                "packageJson": "libs/config/gcloud/package.json",
                "main": "libs/config/gcloud/src/index.ts",
                "assets": ["libs/config/gcloud/*.md"]
            }
        },
        "lint": {
            "executor": "@nrwl/linter:eslint",
            "outputs": ["{options.outputFile}"],
            "options": {
                "lintFilePatterns": ["libs/config/gcloud/**/*.ts"]
            }
        },
        "test": {
            "executor": "@nrwl/jest:jest",
            "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
            "options": {
                "jestConfig": "libs/config/gcloud/jest.config.ts",
                "passWithNoTests": true
            }
        }
    },
    "tags": []
}
