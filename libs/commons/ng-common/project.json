{
    "name": "commons-ng-common",
    "$schema": "../../../node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "libs/commons/ng-common/src",
    "projectType": "library",
    "targets": {
        "build": {
            "executor": "@nrwl/js:tsc",
            "outputs": ["{options.outputPath}"],
            "options": {
                "outputPath": "dist/libs/commons/ng-common",
                "main": "libs/commons/ng-common/src/index.ts",
                "tsConfig": "libs/commons/ng-common/tsconfig.lib.json",
                "assets": ["libs/commons/ng-common/*.md"]
            }
        },
        "lint": {
            "executor": "@nrwl/linter:eslint",
            "outputs": ["{options.outputFile}"],
            "options": {
                "lintFilePatterns": ["libs/commons/ng-common/**/*.ts"]
            }
        }
    },
    "tags": []
}
