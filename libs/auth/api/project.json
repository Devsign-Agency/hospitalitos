{
  "name": "auth-api",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/auth/api/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/libs/auth/api",
        "tsConfig": "libs/auth/api/tsconfig.lib.json",
        "packageJson": "libs/auth/api/package.json",
        "main": "libs/auth/api/src/index.ts",
        "assets": [
          "libs/auth/api/*.md"
        ]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "libs/auth/api/**/*.ts"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "jestConfig": "libs/auth/api/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
